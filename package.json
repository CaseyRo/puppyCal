{
  "name": "puppy-ics",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "dev": "npm test && webpack serve --mode development",
    "dev:server": "webpack serve --mode development",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src --max-warnings 0",
    "lighthouse:ci": "lhci autorun --config=.lighthouserc.json",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm test",
    "ci": "npm run check && npm run build && npm run lighthouse:ci",
    "screenshots": "node scripts/screenshots.mjs",
    "test": "vitest run",
    "version:sync": "node scripts/sync-version.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@lhci/cli": "^0.15.1",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv": "^17.3.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "playwright": "^1.49.0",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.15",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vitest": "^2.1.6",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1"
  }
}
